{
  "name": "frontend",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web && node fix-build.js",
    "export:web": "expo export --platform web",
    "build:railway": "expo export --platform web",
    "start:production": "node server.js",
    "lint": "expo lint",
    "test": "jest",
    "test:unit": "jest --config jest.unit.config.js",
    "test:component": "jest --config jest.component.config.js",
    "test:integration": "jest --config jest.integration.config.js",
    "test:watch": "jest --watch",
    "test:unit:watch": "jest --config jest.unit.config.js --watch",
    "test:component:watch": "jest --config jest.component.config.js --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "type-check": "tsc --noEmit",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:chromium": "playwright test --project=chromium",
    "e2e:firefox": "playwright test --project=firefox",
    "e2e:webkit": "playwright test --project=webkit",
    "e2e:msedge": "playwright test --project=msedge",
    "e2e:mobile": "playwright test --project=\"Mobile Chrome\" --project=\"Mobile Safari\"",
    "e2e:auth": "playwright test auth.spec.ts",
    "e2e:feed": "playwright test feed.spec.ts",
    "e2e:chat": "playwright test chat.spec.ts",
    "e2e:visual": "playwright test visual.spec.ts",
    "e2e:report": "playwright show-report",
    "e2e:install": "playwright install",
    "e2e:install-deps": "playwright install-deps",
    "e2e:codegen": "playwright codegen localhost:8083",
    "test:e2e": "npm run e2e",
    "test:e2e:ci": "playwright test --reporter=junit",
    "test:all": "npm run test:ci && npm run e2e"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@hookform/resolvers": "^5.2.1",
    "@react-native-community/slider": "^5.0.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@tanstack/react-query": "^5.87.1",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.18",
    "expo": "~53.0.22",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-device": "^8.0.8",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-image-manipulator": "^13.1.7",
    "expo-image-picker": "^16.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-linking": "~7.1.7",
    "expo-notifications": "^0.31.4",
    "expo-router": "~5.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "laravel-echo": "^2.2.0",
    "nativewind": "^4.1.23",
    "pusher-js": "^8.4.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.62.0",
    "react-native": "0.79.6",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-gifted-chat": "^2.8.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.13.0",
    "react-native-webview": "13.13.5",
    "zod": "^4.1.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "babel-preset-expo": "^13.2.4",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "jest": "^30.1.3",
    "jest-expo": "^53.0.9",
    "jsdom": "^26.1.0",
    "react-native-web": "^0.21.1",
    "react-test-renderer": "^19.0.0",
    "ts-jest": "^29.4.1",
    "typescript": "~5.8.3"
  },
  "private": true
}
